   "source": [
    "from contextlib import contextmanager, asynccontextmanager\n",
    "\n",
    "@contextmanager\n",
    "def lifespan(var):\n",
    "    print(f\"Read model, allocate resources in context {var}\")\n",
    "    yield\n",
    "    print(f\"Release resources in context {var}\")\n",
    "\n",
    "with lifespan(\"world\"):\n",
    "    print(\"Inside the context\")"
   ]
  }
